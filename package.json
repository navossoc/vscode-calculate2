{
	"name": "calculate2",
	"displayName": "Calculate 2^1",
	"description": "Select some math expression and evaluate it",
	"version": "1.0.0",
	"publisher": "navossoc",
	"engines": {
		"vscode": "^1.31.0"
	},
	"author": {
		"name": "Rafael Cossovan de Fran√ßa",
		"email": "navossoc@gmail.com",
		"url": "https://navossoc.com"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/navossoc/calculate2"
	},
	"icon": "store-icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:calculate2.calculate",
		"onCommand:calculate2.calculateReplace",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "calculate2.calculate",
				"title": "Calculate"
			},
			{
				"command": "calculate2.calculateReplace",
				"title": "Calculate and Replace"
			}
		]
	},
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.3",
		"@types/node": "^12.12.68",
		"@types/vscode": "^1.31.0",
		"@typescript-eslint/eslint-plugin": "^4.4.1",
		"@typescript-eslint/parser": "^4.4.1",
		"eslint": "^7.11.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.0",
		"ts-loader": "^8.0.5",
		"typescript": "^4.0.3",
		"vscode-test": "^1.4.0",
		"webpack": "^5.1.3",
		"webpack-cli": "^4.0.0"
	},
	"dependencies": {
		"@types/mathjs": "^6.0.5",
		"mathjs": "^7.5.1"
	}
}
